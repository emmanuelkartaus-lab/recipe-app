  "editor.showDeprecated": false
}

{
  "editor.showDeprecated": false,
  "editor.semanticTokenColorCustomizations": {
    "enabled": true,
    "rules": {
      "*.deprecated": {
        "strikethrough": false
      }
    }
  }
}
